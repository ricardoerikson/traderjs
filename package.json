{
  "name": "traderjs",
  "version": "0.0.0-semantically-released",
  "description": "Retrieve temporal data about stock market",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "check-coverage": "nyc check-coverage --statements 90 --functions 90 --branches 90 --lines 90",
    "commit": "git-cz",
    "cover": "nyc npm -s t",
    "lint": "eslint src/**/*.js",
    "report-codecov": "nyc report --reporter=text-lcov | codecov",
    "report-coverage": "npm run cover && npm run report-codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha src/test/**/*.test.js --compilers js:babel-register",
    "watch:test": "npm -s t -- -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ricardoerikson/traderjs.git"
  },
  "keywords": [
    "stock",
    "markets",
    "temporal",
    "data",
    "exchange",
    "trading",
    "dataset"
  ],
  "author": "Ricardo Erikson <ricardoerikson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricardoerikson/traderjs/issues"
  },
  "homepage": "https://github.com/ricardoerikson/traderjs#readme",
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.7.1",
    "ghooks": "1.3.2",
    "mocha": "3.1.0",
    "nyc": "8.3.1",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5",
    "semantic-release-cli": "1.4.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run -s lint && npm run -s cover && npm run -s check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "lodash": "4.16.4"
  }
}
